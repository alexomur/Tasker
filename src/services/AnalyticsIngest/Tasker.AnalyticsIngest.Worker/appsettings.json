{
  "Kafka": {
    "Brokers": "redpanda:9092",
    "ClientId": "analytics-ingest"
  },
  "Hdfs": {
    "WebHdfsBaseUrl": "http://namenode:9870",
    "BasePath": "/raw/events",
    "RequestTimeoutSeconds": 30
  },
  "Ingest": {
    "GroupId": "analytics-hdfs-ingest",
    "BatchSize": 200,
    "FlushIntervalSeconds": 5,
    "MaxBufferSize": 5000,
    "RetryDelaySeconds": 5,
    "Topics": [
      "auth.user-registered-v1",
      "auth.user-email-confirmed-v1",
      "auth.user-email-changed-v1",
      "auth.user-display-name-changed-v1",
      "auth.user-password-changed-v1",
      "auth.user-locked-v1",
      "auth.user-unlocked-v1",
      "auth.user-login-succeeded-v1",
      "boardwrite.board-created-v1",
      "boardwrite.board-deleted-v1",
      "boardwrite.board-member-added-v1",
      "boardwrite.board-member-removed-v1",
      "boardwrite.column-created-v1",
      "boardwrite.column-deleted-v1",
      "boardwrite.label-created-v1",
      "boardwrite.label-deleted-v1",
      "boardwrite.card-created-v1",
      "boardwrite.card-updated-v1",
      "boardwrite.card-moved-v1",
      "boardwrite.card-deleted-v1",
      "boardwrite.card-label-attached-v1",
      "boardwrite.card-label-detached-v1",
      "boardwrite.card-assignees-changed-v1",
      "boardwrite.card-due-date-changed-v1"
    ]
  }
}
